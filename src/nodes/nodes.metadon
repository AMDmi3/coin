        if [ "$COIN_EXCLUDE_SOVERTEXSHAPE" = "n" ]; then
          bool '         Exclude SoNonIndexedShape' COIN_EXCLUDE_SONONINDEXEDSHAPE
            if [ "$COIN_EXCLUDE_SONONINDEXEDSHAPE" = "n" ]; then
              bool '            Exclude SoFaceSet' COIN_EXCLUDE_SOFACESET
              bool '            Exclude SoLineSet' COIN_EXCLUDE_SOLINESET
              bool '            Exclude SoPointSet' COIN_EXCLUDE_SOPOINTSET
                if [ "$COIN_EXCLUDE_SOPOINTSET" = "n" ]; then
                  bool '               Exclude SoMarkerSet' COIN_EXCLUDE_SOMARKERSET
                fi
              bool '            Exclude SoQuadMesh' COIN_EXCLUDE_SOQUADMESH
              bool '            Exclude SoTriangleStripSet' COIN_EXCLUDE_SOTRIANGLESTRIPSET
            fi
          bool '         Exclude SoIndexedShape' COIN_EXCLUDE_SOINDEXEDSHAPE
            if [ "$COIN_EXCLUDE_SOINDEXEDSHAPE" = "n" ]; then
              bool '            Exclude SoIndexedFaceSet' COIN_EXCLUDE_SOINDEXEDFACESET
              bool '            Exclude SoIndexedLineSet' COIN_EXCLUDE_SOINDEXEDLINESET
              bool '            Exclude SoIndexedTriangleStripSet' COIN_EXCLUDE_SOINDEXEDTRIANGLESTRIPSET
            fi
        fi
      bool '      Exclude SoImage' COIN_EXCLUDE_SOIMAGE
      bool '      Exclude SoIndexedNurbsCurve' COIN_EXCLUDE_SOINDEXEDNURBSCURVE
      bool '      Exclude SoIndexedNurbsSurface' COIN_EXCLUDE_SOINDEXEDNURBSSURFACE
      bool '      Exclude SoNurbsCurve' COIN_EXCLUDE_SONURBSCURVE
