          bool '         Exclude SoPendulum' COIN_EXCLUDE_SOPENDULUM
          bool '         Exclude SoRotor' COIN_EXCLUDE_SOROTOR
        fi
      bool '      Exclude SoResetTransform' COIN_EXCLUDE_SORESETTRANSFORM
      bool '      Exclude SoRotationXYZ' COIN_EXCLUDE_SOROTATIONXYZ
      bool '      Exclude SoScale' COIN_EXCLUDE_SOSCALE
      bool '      Exclude SoTranslation' COIN_EXCLUDE_SOTRANSLATION
        if [ "$COIN_EXCLUDE_SOTRANSLATION" = "n" ]; then
          bool '         Exclude SoShuttle' COIN_EXCLUDE_SOSHUTTLE
        fi
      bool '      Exclude SoSurroundScale' COIN_EXCLUDE_SOSURROUNDSCALE
      bool '      Exclude SoTransform' COIN_EXCLUDE_SOTRANSFORM
      bool '      Exclude SoUnits' COIN_EXCLUDE_SOUNITS
    fi
  bool '   Exclude SoBaseColor' COIN_EXCLUDE_SOBASECOLOR
  bool '   Exclude SoCallback' COIN_EXCLUDE_SOCALLBACK
  bool '   Exclude SoClipPlane' COIN_EXCLUDE_SOCLIPPLANE
  bool '   Exclude SoColorIndex' COIN_EXCLUDE_SOCOLORINDEX
  bool '   Exclude SoComplexity' COIN_EXCLUDE_SOCOMPLEXITY
  bool '   Exclude SoCoordinate3' COIN_EXCLUDE_SOCOORDINATE3
  bool '   Exclude SoCoordinate4' COIN_EXCLUDE_SOCOORDINATE4
