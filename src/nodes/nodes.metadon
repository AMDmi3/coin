      bool '      Exclude SoLinearProfile' COIN_EXCLUDE_SOLINEARPROFILE
      bool '      Exclude SoNurbsProfile' COIN_EXCLUDE_SONURBSPROFILE
    fi
  bool '   Exclude SoMaterial' COIN_EXCLUDE_SOMATERIAL
  bool '   Exclude SoMaterialBinding' COIN_EXCLUDE_SOMATERIALBINDING
  bool '   Exclude SoNormal' COIN_EXCLUDE_SONORMAL
  bool '   Exclude SoNormalBinding' COIN_EXCLUDE_SONORMALBINDING
  bool '   Exclude SoPackedColor' COIN_EXCLUDE_SOPACKEDCOLOR
  bool '   Exclude SoPickStyle' COIN_EXCLUDE_SOPICKSTYLE
  bool '   Exclude SoPolygonOffset' COIN_EXCLUDE_SOPOLYGONOFFSET
  bool '   Exclude SoProfileCoordinate2' COIN_EXCLUDE_SOPROFILECOORDINATE2
  bool '   Exclude SoProfileCoordinate3' COIN_EXCLUDE_SOPROFILECOORDINATE3
  bool '   Exclude SoShapeHints' COIN_EXCLUDE_SOSHAPEHINTS
  bool '   Exclude SoTexture2' COIN_EXCLUDE_SOTEXTURE2
  bool '   Exclude SoTexture2Transform' COIN_EXCLUDE_SOTEXTURE2TRANSFORM
  bool '   Exclude SoTextureCoordinate2' COIN_EXCLUDE_SOTEXTURECOORDINATE2
  bool '   Exclude SoTextureCoordinateBinding' COIN_EXCLUDE_SOTEXTURECOORDINATEBINDING
  bool '   Exclude SoTextureCoordinateFunction' COIN_EXCLUDE_SOTEXTURECOORDINATEFUNCTION
    if [ "$COIN_EXCLUDE_SOTEXTURECOORDINATEFUNCTION" = "n" ]; then
      bool '      Exclude SoTextureCoordinateDefault' COIN_EXCLUDE_SOTEXTURECOORDINATEDEFAULT
      bool '      Exclude SoTextureCoordinateEnvironment' COIN_EXCLUDE_SOTEXTURECOORDINATEENVIRONMENT
      bool '      Exclude SoTextureCoordinatePlane' COIN_EXCLUDE_SOTEXTURECOORDINATEPLANE
    fi
  bool '   Exclude SoVertexProperty' COIN_EXCLUDE_SOVERTEXPROPERTY
  bool '   Exclude SoWWWInline' COIN_EXCLUDE_SOWWWINLINE
fi
