      bool '      Exclude SoSphere' COIN_EXCLUDE_SOSPHERE
      bool '      Exclude SoText2' COIN_EXCLUDE_SOTEXT2
      bool '      Exclude SoText3' COIN_EXCLUDE_SOTEXT3
    fi
  bool '   Exclude SoAnnoText3Property' COIN_EXCLUDE_SOANNOTEXT3PROPERTY
  bool '   Exclude SoGroup' COIN_EXCLUDE_SOGROUP
    if [ "$COIN_EXCLUDE_SOGROUP" = "n" ]; then
      bool '      Exclude SoSeparator' COIN_EXCLUDE_SOSEPARATOR
        if [ "$COIN_EXCLUDE_SOSEPARATOR" = "n" ]; then
          bool '         Exclude SoAnnotation' COIN_EXCLUDE_SOANNOTATION
          bool '         Exclude SoSelection' COIN_EXCLUDE_SOSELECTION
            if [ "$COIN_EXCLUDE_SOSELECTION" = "n" ]; then
              bool '            Exclude SoExtSelection' COIN_EXCLUDE_SOEXTSELECTION
            fi
          bool '         Exclude SoLocateHighlight' COIN_EXCLUDE_SOLOCATEHIGHLIGHT
            if [ "$COIN_EXCLUDE_SOSELECTION" = "n" ]; then
              bool '            Exclude SoWWWAnchor' COIN_EXCLUDE_SOWWWANCHOR
            fi
        fi
      bool '      Exclude SoArray' COIN_EXCLUDE_SOARRAY
      bool '      Exclude SoSwitch' COIN_EXCLUDE_SOSWITCH
        if [ "$COIN_EXCLUDE_SOSWITCH" = "n" ]; then
          bool '         Exclude SoBlinker' COIN_EXCLUDE_SOBLINKER
        fi        
      bool '      Exclude SoLOD' COIN_EXCLUDE_SOLOD
        if [ "$COIN_EXCLUDE_SOLOD" = "n" ]; then
          bool '         Exclude SoLevelOfSimplification' COIN_EXCLUDE_SOLEVELOFSIMPLIFICATION
        fi        
      bool '      Exclude SoLevelOfDetail' COIN_EXCLUDE_SOLEVELOFDETAIL
      bool '      Exclude SoMultipleCopy' COIN_EXCLUDE_SOMULTIPLECOPY
      bool '      Exclude SoPathSwitch' COIN_EXCLUDE_SOPATHSWITCH
      bool '      Exclude SoTransformSeparator' COIN_EXCLUDE_SOTRANSFORMSEPARATOR
    fi
  bool '   Exclude SoTransformation' COIN_EXCLUDE_SOTRANSFORMATION
