#############################################################################

bool 'Disable nodes' COIN_EXCLUDE_SONODE
if [ "$COIN_EXCLUDE_SONODE" = "n" ]; then
  bool '   Exclude SoCamera' COIN_EXCLUDE_SOCAMERA
    if [ "$COIN_EXCLUDE_SOCAMERA" = "n" ]; then
      bool '      Exclude SoPerspectiveCamera' COIN_EXCLUDE_SOPERSPECTIVECAMERA
      bool '      Exclude SoOrthographicCamera' COIN_EXCLUDE_SOORTHOGRAPHICCAMERA
    fi
  bool '   Exclude SoShape' COIN_EXCLUDE_SOSHAPE
