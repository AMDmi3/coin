    if [ "$COIN_EXCLUDE_SOLIGHT" = "n" ]; then
      bool '      Exclude SoDirectionalLight' COIN_EXCLUDE_SODIRECTIONALLIGHT
      bool '      Exclude SoSpotLight' COIN_EXCLUDE_SOSPOTLIGHT
      bool '      Exclude SoPointLight' COIN_EXCLUDE_SOPOINTLIGHT
    fi
  bool '   Exclude SoDrawStyle' COIN_EXCLUDE_SODRAWSTYLE
  bool '   Exclude SoEnvironment' COIN_EXCLUDE_SOENVIRONMENT
  bool '   Exclude SoEventCallback' COIN_EXCLUDE_SOEVENTCALLBACK
  bool '   Exclude SoFile' COIN_EXCLUDE_SOFILE
