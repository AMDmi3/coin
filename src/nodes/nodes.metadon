      bool '      Exclude SoAnnoText3' COIN_EXCLUDE_SOANNOTEXT3
      bool '      Exclude SoAsciiText' COIN_EXCLUDE_SOASCIITEXT
      bool '      Exclude SoCone' COIN_EXCLUDE_SOCONE
      bool '      Exclude SoCube' COIN_EXCLUDE_SOCUBE
      bool '      Exclude SoCylinder' COIN_EXCLUDE_SOCYLINDER
