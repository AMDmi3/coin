	@list='$(libdraggersLINKHACKinc_HEADERS)'; for p in $$list; do \
	  if test -f "$$p"; then d= ; else d="$(srcdir)/"; fi; \
	  f="`echo $$p | sed -e 's|^.*/||'`"; \
	  if cmp -s $$d$$p $(DESTDIR)$(libdraggersLINKHACKincdir)/$$f; then :; else echo "Installing $(libdraggersLINKHACKincdir)/$$f"; $(INSTALL_DATA) $$d$$p $(DESTDIR)$(libdraggersLINKHACKincdir)/$$f; fi \
	done

uninstall-libdraggersLINKHACKincHEADERS:
	@$(NORMAL_UNINSTALL)
	@list='$(libdraggersLINKHACKinc_HEADERS)'; for p in $$list; do \
	  f="`echo $$p | sed -e 's|^.*/||'`"; \
	  echo "Uninstalling $(DESTDIR)$(libdraggersLINKHACKincdir)/$$f"; \
	  rm -f $(DESTDIR)$(libdraggersLINKHACKincdir)/$$f; \
	done

